{
  "productName": "wen_tools",
  "version": "../package.json",
  "identifier": "wen.suwenmao.wentools",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:4096",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "label": "main",
        "url": "/",
        "center": true,
        "x": 0,
        "y": 0,
        "width": 1080,
        "height": 640,
        "minWidth": 640,
        "minHeight": 400,
        "maxWidth": 1920,
        "maxHeight": 1366,
        "resizable": true,
        "title": "wen_tools",
        "fullscreen": false,
        "focus": true,
        "transparent": false,
        "maximizable": true,
        "visible": true,
        "decorations": false,
        "alwaysOnTop": false,
        "contentProtected": false,
        "skipTaskbar": false,
        "theme": "Light",
        "titleBarStyle": "Visible",
        "hiddenTitle": false,
        "acceptFirstMouse": false,
        "alwaysOnBottom": false,
        "closable": true,
        "dragDropEnabled": true,
        "maximized": false,
        "shadow": true
      }
    ],
    "security": {
      "assetProtocol": {
        "enable": false,
        "scope": []
      },
      "capabilities": [],
      "dangerousDisableAssetCspModification": false,
      "freezePrototype": false,
      "csp": null,
      "pattern": {
        "use": "brownfield"
      }
    },
    "trayIcon": {
      "iconAsTemplate": false,
      "iconPath": "icons/icon.png",
      "id": "wen-icon",
      "title": "wen_tools",
      "menuOnLeftClick": true,
      "tooltip": "a tools | 一个 工具箱"
    },
    "macOSPrivateApi": false
  },
  "bundle": {
    "active": true,
    "android": {
      "minSdkVersion": 24,
      "versionCode": 1
    },
    "category": "DeveloperTool",
    "copyright": "",
    "createUpdaterArtifacts": "v1Compatible",
    "icon": [
      "icons/android/mipmap-hdpi/ic_launcher_foreground.png",
      "icons/android/mipmap-hdpi/ic_launcher_round.png",
      "icons/android/mipmap-hdpi/ic_launcher.png",
      "icons/android/mipmap-mdpi/ic_launcher_foreground.png",
      "icons/android/mipmap-mdpi/ic_launcher_round.png",
      "icons/android/mipmap-mdpi/ic_launcher.png",
      "icons/android/mipmap-xhdpi/ic_launcher_foreground.png",
      "icons/android/mipmap-xhdpi/ic_launcher_round.png",
      "icons/android/mipmap-xhdpi/ic_launcher.png",
      "icons/android/mipmap-xxhdpi/ic_launcher_foreground.png",
      "icons/android/mipmap-xxhdpi/ic_launcher_round.png",
      "icons/android/mipmap-xxhdpi/ic_launcher.png",
      "icons/android/mipmap-xxxhdpi/ic_launcher_foreground.png",
      "icons/android/mipmap-xxxhdpi/ic_launcher_round.png",
      "icons/android/mipmap-xxxhdpi/ic_launcher.png",

      "icons/ios/AppIcon-20x20@1x.png",
      "icons/ios/AppIcon-20x20@2x-1.png",
      "icons/ios/AppIcon-20x20@2x.png",
      "icons/ios/AppIcon-20x20@3x.png",
      "icons/ios/AppIcon-29x29@1x.png",
      "icons/ios/AppIcon-29x29@2x-1.png",
      "icons/ios/AppIcon-29x29@2x.png",
      "icons/ios/AppIcon-29x29@3x.png",
      "icons/ios/AppIcon-40x40@1x.png",
      "icons/ios/AppIcon-40x40@2x-1.png",
      "icons/ios/AppIcon-40x40@2x.png",
      "icons/ios/AppIcon-40x40@3x.png",
      "icons/ios/AppIcon-60x60@2x.png",
      "icons/ios/AppIcon-60x60@3x.png",
      "icons/ios/AppIcon-76x76@1x.png",
      "icons/ios/AppIcon-76x76@2x.png",
      "icons/ios/AppIcon-83.5x83.5@2x.png",
      "icons/ios/AppIcon-512@2x.png",

      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico",
      "icons/icon.png",
      "icons/Square30x30Logo.png",
      "icons/Square44x44Logo.png",
      "icons/Square71x71Logo.png",
      "icons/Square89x89Logo.png",
      "icons/Square107x107Logo.png",
      "icons/Square142x142Logo.png",
      "icons/Square150x150Logo.png",
      "icons/Square284x284Logo.png",
      "icons/Square310x310Logo.png",
      "icons/StoreLogo.png"
    ],
    "linux": {
      "appimage": {
        "bundleMediaFramework": false,
        "files": {}
      },
      "deb": {
        "files": {}
      },
      "rpm": {
        "epoch": 0,
        "files": {},
        "release": "1"
      }
    },
    "licenseFile": "wen/license.txt",
    "longDescription": "一个普普通通的工具箱程序，全力开发中 | An ordinary toolbox program,In full development",
    "publisher": "suwenmao",
    "shortDescription": "一个普普通通的工具箱程序 | An ordinary toolbox program",
    "targets": "all",
    "windows": {
      "allowDowngrades": true,
      "webviewFixedRuntimePath": null,
      "webviewInstallMode": {
        "silent": true,
        "type": "offlineInstaller"
      },
      "nsis": {
        "compression": "lzma",
        "displayLanguageSelector": true,
        "headerImage": "wen/headimges.bmp",
        "installerIcon": "icons/icon.ico",
        "installMode": "both",
        "sidebarImage": "wen/sidebarImage.bmp",
        "languages": [
          "English",
          "TradChinese",
          "SimpChinese",
          "Japanese"
        ]
      },
      "wix": {
        "bannerPath": "wen/bannerPath.bmp",
        "dialogImagePath": "wen/dialogImagesPath.bmp",
        "language": [
          "en-US",
          "zh-CN",
          "zh-TW",
          "jp-JP"
        ]
      }
    }
  },
  "plugins": {
    "cli": {
      "description": "wen_tools Help guide",
      "longDescription": "A little help guide",
      "beforeHelp": "wen_tools app",
      "afterHelp": "A simple application",
      "args": [
        {
          "short": "v",
          "name": "verbose",
          "description": "Verbosity level"
        }
      ],
      "subcommands": {
        "run": {
          "description": "Run the application",
          "args": [
            {
              "name": "debug",
              "description": "Run application in debug mode"
            },
            {
              "name": "release",
              "description": "Run application in release mode"
            }
          ]
        }
      }
    },
    "updater": {
      "pubkey": "",
      "windows": {
        "installMode": "passive",
        "installerArgs": []
      },
      "endpoints": []
    }
  },
  "$schema": "../node_modules/@tauri-apps/cli/schema.json"
}
